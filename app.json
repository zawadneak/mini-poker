{
  "expo": {
    "name": "JustPoker",
    "scheme": "JustPoker",
    "slug": "JustPoker",
    "version": "0.0.7-alpha.1",
    "orientation": "portrait",
    "icon": "./assets/branding/just-poker.png",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "./assets/branding/curi-white.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "assetBundlePatterns": ["**/*"],
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      }
    },
    "web": {
      "favicon": "./web/favicon.svg",
      "bundler": "metro"
    }
  }
}
